{
    "manifest_version": 2,
    "name": "proxy setup",
    "description": "connect your own proxy to chrome",
    "version": "1.0.6",
    "background": {
        "scripts": [
            "background.js"
        ]
    },
    "permissions": [
        "activeTab",
        "storage",
        "proxy",
        "tabs",
        "webRequest",
        "webRequestBlocking",
        "background"
    ],
    "icons": {
        "128": "icon.png"
    },
    "browser_action": {
        "default_icon": "icon.png",
        "default_popup": "popup.html"
    },
    "content_scripts": [
        {
            "matches": [
                "<all_urls>"
            ],
            "js": [
                "content.js"
            ]
        }
    ]
}